# Theme author
ThemeAuthor="lunarplums"

# Theme description
ThemeDescription="Windows Standard [Version 5.00.2195]"

# Look
Look=pixmap

# Horizontal dialog window border
DlgBorderSizeX=3 # 0-128

# Vertical dialog window border
DlgBorderSizeY=3 # 0-128

# Horizontal window border
BorderSizeX=3 # 0-128

# Vertical window border
BorderSizeY=3 # 0-128

# Resize corner width
CornerSizeX=22 # 0-64

# Resize corner height
CornerSizeY=22 0-64

# Shaped pixmaps protect client window
ShapesProtectClientWindow=1 # 0 / 1

# Fill highlight rectangle
QuickSwitchFillSelection=0 # 0 / 1

# Show text above icons
QuickSwitchTextFirst=0 # 0 / 1

# Border size of quick-switch icons
QuickSwitchIconBorder=4 # 0-30

# Size of icon margins of quick-switch icons
QuickSwitchIconMargin=4 # 0-30

# Horizontal margin of quick-switch window
QuickSwitchHorzMargin=0 # 0-30

# Vertical margin of quick-switch window
QuickSwitchVertMargin=10 # 0-30

# Size of the quick-switch window separator
QuickSwitchSeparatorSize=20 0-30

#Surround the tray with plastic border.
TrayDrawBevel=1 # 0/1

#Maximum scaled height of tray icons.
TrayIconMaxHeight=21 # [16-128]

#Maximum scaled width of tray icons.
TrayIconMaxWidth=21 # [16-128]

# Horizontal scrollbar button size
ScrollBarX=16 # 0-100

# Vertical scrollbar button size
ScrollBarY=16 # 0-100

# Menu icon size
MenuIconSize=12 # 8-128

# Dimension of the small icons
SmallIconSize=16 # 8-128

# Dimension of the large icons
LargeIconSize=32 # 8-128

# Dimension of the extra large icons
HugeIconSize=32 # 8-128

#Display Menu Icon
ShowMenuButtonIcon=1

# Does the theme support the "O" title bar button images (for mouse rollover)
RolloverButtonsSupported=1 # 0 / 1

# Titlebar buttons supported by theme (x, m, i, r, h, s, d)
TitleButtonsSupported="shxmi"

# Titlebar buttons from left to right (x=close, m=max, i=min, h=hide, r=rollup, s=sysmenu)
TitleButtonsLeft="s"

# Titlebar buttons from right to left (x=close, m=max, i=min, h=hide, r=rollup, s=sysmenu)
TitleButtonsRight="xmi"

# Title bar height
TitleBarHeight=19 # 0-128

# Title bar join right
TitleBarJoinRight=1 # 0 / 1

# Title bar join left
TitleBarJoinLeft=0 # 0 / 1

# Title bar text position
TitleBarJustify=0 # 0-100

# Title bar text horizontal offset
TitleBarHorzOffset=0 # -128-128

# Title bar text vertical offset
TitleBarVertOffset=0 # -128-128

# List of gradient images in the current theme
Gradients="titleAB.xpm titleIB.xpm titleAT.xpm titleIT.xpm"

